{
  "name": "@ave-id/embed",
  "version": "0.2.5",
  "type": "module",
  "main": "dist/embed.js",
  "module": "dist/embed.js",
  "types": "dist/embed.d.ts",
  "exports": {
    ".": {
      "types": "./dist/embed.d.ts",
      "import": "./dist/embed.js",
      "default": "./dist/embed.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "mkdir -p dist && cp src/embed.js dist/embed.js && cp src/embed.d.ts dist/embed.d.ts",
    "prepublishOnly": "npm run build"
  }
}
